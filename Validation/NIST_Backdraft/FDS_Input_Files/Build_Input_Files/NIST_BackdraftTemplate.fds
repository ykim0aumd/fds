&HEAD CHID='paramCHID',TITLE='paramTITLE' /

! NIST_backdraft imput files template.

&TIME T_END=20. /
&MISC SUBGRID_IGNITION_MODEL=F/
&CLIP MINIMUM_DENSITY=0.01/

&VENT MB='XMIN',SURF_ID='OPEN' /
&VENT MB='XMAX',SURF_ID='OPEN' /
&VENT MB='YMIN',SURF_ID='OPEN' /
&VENT MB='YMAX',SURF_ID='OPEN' /
&VENT MB='ZMAX',SURF_ID='OPEN' /

&SPEC ID='NITROGEN', MASS_FRACTION_0=0.77, BACKGROUND=.TRUE./
&SPEC ID='param6' /
&SPEC ID='OXYGEN', MASS_FRACTION_0=0.23 /
&SPEC ID='CARBON MONOXIDE' /
&SPEC ID='WATER VAPOR' /
&SPEC ID='CARBON DIOXIDE' /
&SPEC ID='SOOT' /

! Here we read mesh file, ignitor position file, and reaction file.
&CATF OTHER_FILES='param3','param4','param5' /

! Initialize the whole domain:
&INIT XB=-1.824,1.824,-1.11,1.11,-1.00,3.00, MASS_FRACTION(1)=0.23, SPEC_ID(1)='OXYGEN' /

! Two zone initialization in compartment, Z_bound=0.3925 m.
&INIT XB=-1.52,0.,-0.495,0.495,0.0,0.3925, 
      VOLUME_FRACTION(1)=param15, SPEC_ID(1)='param6', 
      VOLUME_FRACTION(2)=param16, SPEC_ID(2)='OXYGEN',
      VOLUME_FRACTION(3)=param17, SPEC_ID(3)='CARBON DIOXIDE', 
      VOLUME_FRACTION(4)=param18, SPEC_ID(4)='CARBON MONOXIDE',
      VOLUME_FRACTION(5)=param19, SPEC_ID(5)='WATER VAPOR',
      TEMPERATURE=param14 /

&INIT XB=-1.52,0.,-0.495,0.495,0.3925,0.99, 
      VOLUME_FRACTION(1)=param8, SPEC_ID(1)='param6', 
      VOLUME_FRACTION(2)=param9, SPEC_ID(2)='OXYGEN',
      VOLUME_FRACTION(3)=param10, SPEC_ID(3)='CARBON DIOXIDE', 
      VOLUME_FRACTION(4)=param11, SPEC_ID(4)='CARBON MONOXIDE',
      VOLUME_FRACTION(5)=param12, SPEC_ID(5)='WATER VAPOR',
      TEMPERATURE=param7 /


&SURF ID='Compartment Wall', COLOR='SILVER', ADIABATIC=T, HEAT_TRANSFER_COEFFICIENT=0./
c
# Compartment:
&OBST ID='Compartment floor', XB=-1.55, 0.03,-0.515, 0.515,-0.03,0.00, SURF_ID='Compartment Wall' /
&OBST ID='Compartment right', XB=-1.55, 0.03,-0.515,-0.495, 0.00,0.99, SURF_ID='Compartment Wall' /
&OBST ID='Compartment left',  XB=-1.55, 0.03, 0.495, 0.515, 0.00,0.99, SURF_ID='Compartment Wall' /
&OBST ID='Compartment back',  XB=-1.55,-1.52,-0.495, 0.495, 0.00,0.99, SURF_ID='Compartment Wall' /
&OBST ID='Compartment front', XB= 0.00, 0.03,-0.495, 0.495, 0.00,0.99, SURF_ID='Compartment Wall' /
&OBST ID='Compartment roof',  XB=-1.55, 0.03,-0.515, 0.515, 0.99,1.02, SURF_ID='Compartment Wall' /

# Door entrance 43x80 cm:
&HOLE ID='Compartment door', XB= -0.01  , 0.04,-0.215, 0.215, 0.00,0.80, COLOR='GRAY'/

# Door opened:
&OBST ID='Open door',  XB= 0.0608, 0.4808,-0.245,-0.215, 0.00,0.80, SURF_ID='Compartment Wall'/

# Burner:
&OBST ID='Burner', XB=-1.46,-1.282,-0.089, 0.089, 0.088,0.24, SURF_ID='Compartment Wall' /

# Devcs Lower Ignitor:
&DEVC ID='MO1', XYZ=-1.095,0.015,.255, QUANTITY='MASS FRACTION', SPEC_ID='OXYGEN'/
&DEVC ID='VO1', XYZ=-1.095,0.015,.255, QUANTITY='VOLUME FRACTION', SPEC_ID='OXYGEN'/
&DEVC ID='TP1', XYZ=-1.095,0.015,.255, QUANTITY='TEMPERATURE'/
&DEVC ID='HR1', XYZ=-1.095,0.015,.255, QUANTITY='HRRPUV REAC', REAC_ID='REAC1'/

# Devcs Mid Ignitor:
&DEVC ID='MO2', XYZ=-1.095,0.015,.515, QUANTITY='MASS FRACTION', SPEC_ID='OXYGEN'/
&DEVC ID='VO2', XYZ=-1.095,0.015,.515, QUANTITY='VOLUME FRACTION', SPEC_ID='OXYGEN'/
&DEVC ID='TP2', XYZ=-1.095,0.015,.515, QUANTITY='TEMPERATURE'/
&DEVC ID='HR2', XYZ=-1.095,0.015,.515, QUANTITY='HRRPUV REAC', REAC_ID='REAC1'/

&SLCF PBY=0., QUANTITY='VELOCITY', VECTOR=T/
&SLCF PBY=0.015, QUANTITY='HRRPUV', CELL_CENTERED=T/
&SLCF PBY=0.015, QUANTITY='TEMPERATURE', CELL_CENTERED=T/
&SLCF PBY=0.015, QUANTITY='MASS FRACTION', SPEC_ID='param6', CELL_CENTERED=T/
&SLCF PBY=0.015, QUANTITY='MASS FRACTION', SPEC_ID='NITROGEN', CELL_CENTERED=T/
&SLCF PBY=0.015, QUANTITY='MASS FRACTION', SPEC_ID='OXYGEN', CELL_CENTERED=T/
&SLCF PBY=0.015, QUANTITY='MASS FRACTION', SPEC_ID='CARBON MONOXIDE', CELL_CENTERED=T/
&SLCF PBY=0.015, QUANTITY='EXTINCTION', CELL_CENTERED=T/
&SLCF PBY=0.015, QUANTITY='AUTO IGNITION TEMPERATURE', CELL_CENTERED=T/
&SLCF PBY=0.015, QUANTITY='PRESSURE', CELL_CENTERED=T/

# Devcs results:
# Compute total HRR outside of compartment:
&DEVC ID='HRR REAC1', XB=0.03,1.824,-1.11,1.11,-1.00,3.00, QUANTITY='HRRPUV REAC', REAC_ID='REAC1', SPATIAL_STATISTIC='VOLUME INTEGRAL' /
&DEVC ID='HRR REAC2', XB=0.03,1.824,-1.11,1.11,-1.00,3.00, QUANTITY='HRRPUV REAC', REAC_ID='REAC2', SPATIAL_STATISTIC='VOLUME INTEGRAL' /
&CTRL ID='SUMHRR_REAC_CTRL', FUNCTION_TYPE='SUM', INPUT_ID='HRR REAC1','HRR REAC2'/
&DEVC ID='SUMHRR REAC', XYZ=0,0,0, QUANTITY='CONTROL VALUE', CTRL_ID='SUMHRR_REAC_CTRL', UNITS='kW' /

# Pressure tap:
&DEVC ID='Pres1', XYZ=-0.7,-0.47,0.495, QUANTITY='PRESSURE'/

# Total Fuel mass in the compartment:
&DEVC ID='FuelMass', XB=-1.52,0.0,-0.5,0.5,0.0,0.99, QUANTITY='DENSITY', SPEC_ID='param6', SPATIAL_STATISTIC='VOLUME INTEGRAL' /

# Thermocouples, use Inconel 600, and sheath diameter:
# From https://asm.matweb.com/search/SpecificMaterial.asp?bassnum=NINC30:
&PROP ID='TC', DENSITY=8470, DIAMETER=0.0032, SPECIFIC_HEAT=0.444/
&DEVC ID='T0', QUANTITY='THERMOCOUPLE', PROP_ID='TC', XYZ=-0.62,0.26,0.79 /
&DEVC ID='T1', QUANTITY='THERMOCOUPLE', PROP_ID='TC', XYZ=-0.62,0.26,0.59 /
&DEVC ID='T2', QUANTITY='THERMOCOUPLE', PROP_ID='TC', XYZ=-0.62,0.26,0.394/
&DEVC ID='T3', QUANTITY='THERMOCOUPLE', PROP_ID='TC', XYZ=-0.62,0.26,0.197 /
&DEVC ID='T4', QUANTITY='THERMOCOUPLE', PROP_ID='TC', XYZ=-0.375,-0.02,0.94 /
&DEVC ID='T5', QUANTITY='THERMOCOUPLE', PROP_ID='TC', XYZ=-0.375,-0.02,0.24 /
&DEVC ID='T6', QUANTITY='THERMOCOUPLE', PROP_ID='TC', XYZ=-0.86 ,-0.02,0.94 /
&DEVC ID='T7', QUANTITY='THERMOCOUPLE', PROP_ID='TC', XYZ=-0.86 ,-0.02,0.24 /

# Gas sampling devices:
# Fuel:
&DEVC ID='F_1', XYZ=-0.375,0.0,0.90,  QUANTITY='VOLUME FRACTION', SPEC_ID='param6'/
&DEVC ID='F_2', XYZ=-1.11 ,0.0,0.56,  QUANTITY='VOLUME FRACTION', SPEC_ID='param6'/
&DEVC ID='F_3', XYZ=-0.375,0.0,0.495, QUANTITY='VOLUME FRACTION', SPEC_ID='param6'/
&DEVC ID='F_4', XYZ=-1.11 ,0.0,0.46,  QUANTITY='VOLUME FRACTION', SPEC_ID='param6'/
&DEVC ID='F_5', XYZ=-1.11,0.0,0.325,  QUANTITY='VOLUME FRACTION', SPEC_ID='param6'/
&DEVC ID='F_6', XYZ=-1.11,0.0,0.22,   QUANTITY='VOLUME FRACTION', SPEC_ID='param6'/

# Oxygen:
&DEVC ID='O2_1', XYZ=-0.375,0.0,0.90,  QUANTITY='VOLUME FRACTION', SPEC_ID='OXYGEN'/
&DEVC ID='O2_2', XYZ=-1.11 ,0.0,0.56,  QUANTITY='VOLUME FRACTION', SPEC_ID='OXYGEN'/
&DEVC ID='O2_3', XYZ=-0.375,0.0,0.495, QUANTITY='VOLUME FRACTION', SPEC_ID='OXYGEN'/
&DEVC ID='O2_4', XYZ=-1.11 ,0.0,0.46,  QUANTITY='VOLUME FRACTION', SPEC_ID='OXYGEN'/
&DEVC ID='O2_5', XYZ=-1.11,0.0,0.325,  QUANTITY='VOLUME FRACTION', SPEC_ID='OXYGEN'/
&DEVC ID='O2_6', XYZ=-1.11,0.0,0.22,   QUANTITY='VOLUME FRACTION', SPEC_ID='OXYGEN'/

# Nitrogen:
&DEVC ID='N2_1', XYZ=-0.375,0.0,0.90,  QUANTITY='VOLUME FRACTION', SPEC_ID='NITROGEN'/
&DEVC ID='N2_2', XYZ=-1.11 ,0.0,0.56,  QUANTITY='VOLUME FRACTION', SPEC_ID='NITROGEN'/
&DEVC ID='N2_3', XYZ=-0.375,0.0,0.495, QUANTITY='VOLUME FRACTION', SPEC_ID='NITROGEN'/
&DEVC ID='N2_4', XYZ=-1.11 ,0.0,0.46,  QUANTITY='VOLUME FRACTION', SPEC_ID='NITROGEN'/
&DEVC ID='N2_5', XYZ=-1.11,0.0,0.325,  QUANTITY='VOLUME FRACTION', SPEC_ID='NITROGEN'/
&DEVC ID='N2_6', XYZ=-1.11,0.0,0.22,   QUANTITY='VOLUME FRACTION', SPEC_ID='NITROGEN'/

# CO:
&DEVC ID='CO_1', XYZ=-0.375,0.0,0.90,  QUANTITY='VOLUME FRACTION', SPEC_ID='CARBON MONOXIDE'/
&DEVC ID='CO_2', XYZ=-1.11 ,0.0,0.56,  QUANTITY='VOLUME FRACTION', SPEC_ID='CARBON MONOXIDE'/
&DEVC ID='CO_3', XYZ=-0.375,0.0,0.495, QUANTITY='VOLUME FRACTION', SPEC_ID='CARBON MONOXIDE'/
&DEVC ID='CO_4', XYZ=-1.11 ,0.0,0.46,  QUANTITY='VOLUME FRACTION', SPEC_ID='CARBON MONOXIDE'/
&DEVC ID='CO_5', XYZ=-1.11,0.0,0.325,  QUANTITY='VOLUME FRACTION', SPEC_ID='CARBON MONOXIDE'/
&DEVC ID='CO_6', XYZ=-1.11,0.0,0.22,   QUANTITY='VOLUME FRACTION', SPEC_ID='CARBON MONOXIDE'/

# CO2:
&DEVC ID='CO2_1', XYZ=-0.375,0.0,0.90,  QUANTITY='VOLUME FRACTION', SPEC_ID='CARBON DIOXIDE'/
&DEVC ID='CO2_2', XYZ=-1.11 ,0.0,0.56,  QUANTITY='VOLUME FRACTION', SPEC_ID='CARBON DIOXIDE'/
&DEVC ID='CO2_3', XYZ=-0.375,0.0,0.495, QUANTITY='VOLUME FRACTION', SPEC_ID='CARBON DIOXIDE'/
&DEVC ID='CO2_4', XYZ=-1.11 ,0.0,0.46,  QUANTITY='VOLUME FRACTION', SPEC_ID='CARBON DIOXIDE'/
&DEVC ID='CO2_5', XYZ=-1.11,0.0,0.325,  QUANTITY='VOLUME FRACTION', SPEC_ID='CARBON DIOXIDE'/
&DEVC ID='CO2_6', XYZ=-1.11,0.0,0.22,   QUANTITY='VOLUME FRACTION', SPEC_ID='CARBON DIOXIDE'/

# H2O:
&DEVC ID='H2O_1', XYZ=-0.375,0.0,0.90,  QUANTITY='VOLUME FRACTION', SPEC_ID='WATER VAPOR'/
&DEVC ID='H2O_2', XYZ=-1.11 ,0.0,0.56,  QUANTITY='VOLUME FRACTION', SPEC_ID='WATER VAPOR'/
&DEVC ID='H2O_3', XYZ=-0.375,0.0,0.495, QUANTITY='VOLUME FRACTION', SPEC_ID='WATER VAPOR'/
&DEVC ID='H2O_4', XYZ=-1.11 ,0.0,0.46,  QUANTITY='VOLUME FRACTION', SPEC_ID='WATER VAPOR'/
&DEVC ID='H2O_5', XYZ=-1.11,0.0,0.325,  QUANTITY='VOLUME FRACTION', SPEC_ID='WATER VAPOR'/
&DEVC ID='H2O_6', XYZ=-1.11,0.0,0.22,   QUANTITY='VOLUME FRACTION', SPEC_ID='WATER VAPOR'/

# Heat Flux Gauge, in the compartment floor, looking up:
&DEVC ID='HFG', XYZ=-0.355,0.0,0.01, QUANTITY='GAUGE HEAT FLUX', IOR=3, PROP_ID='hfp'/
&PROP ID='hfp', GAUGE_TEMPERATURE=25., GAUGE_EMISSIVITY=0.9 /

&TAIL/


