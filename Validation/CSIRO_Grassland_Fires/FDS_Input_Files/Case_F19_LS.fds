&HEAD CHID='Case_F19_LS', TITLE='Australian Grassland Fire Experiment, Case F19' /

&MESH IJK=48,48,8, XB=-20.0,220.0,-120.0,120.0,0.0,40.0 /

&TIME T_END=180. /

&MISC SIMULATION_MODE='SVLES', TMPA=34., LEVEL_SET_MODE=4 /

&WIND SPEED=4.8, DIRECTION=270., L=-500., Z_0=0.03 /

&REAC FUEL='FUEL VAPOR', C=3.4, H=6.2, O=2.5, SOOT_YIELD=0.02, HEAT_OF_COMBUSTION=17700. /

&SURF ID='IGNITION', VEG_LSET_IGNITE_TIME=0. /
&SURF ID='LINE', VEG_LSET_ROS_00 = 1./

&VENT XB=-5.0,0.0, -5., 5.,0.,0., SURF_ID='IGNITION' /
&VENT XB=-5.0,0.0,-90.,-5.,0.,0., SURF_ID='LINE' /
&VENT XB=-5.0,0.0,  5.,90.,0.,0., SURF_ID='LINE' /
&VENT XB= 0.0,200.0,-100.,100.,0.,0., SURF_ID='Tall Grass' /

&VENT MB='XMIN', SURF_ID='OPEN' /
&VENT MB='XMAX', SURF_ID='OPEN' /
&VENT MB='YMIN', SURF_ID='OPEN' /
&VENT MB='YMAX', SURF_ID='OPEN' /
&VENT MB='ZMAX', SURF_ID='OPEN' /

&SURF ID='ROAD', COLOR='BEIGE' /

&VENT XB=-20,220,-105,-100,0,0, SURF_ID='ROAD' /
&VENT XB=-20,220,-115,-110,0,0, SURF_ID='ROAD' /
&VENT XB=-20,220, 100, 105,0,0, SURF_ID='ROAD' /
&VENT XB=-20,220, 110, 115,0,0, SURF_ID='ROAD' /
&VENT XB= -5,  0,-120, 120,0,0, SURF_ID='ROAD' /
&VENT XB=-15,-10,-120, 120,0,0, SURF_ID='ROAD' /
&VENT XB=200,205,-120, 120,0,0, SURF_ID='ROAD' /
&VENT XB=210,215,-120, 120,0,0, SURF_ID='ROAD' /

&SURF ID                        = 'Tall Grass'
      VEG_LSET_FUEL_INDEX       = 3
      VEG_LSET_M1               = 0.058 /

&DUMP DT_HRR=1., DT_DEVC=1., DT_SLCF=1., SIG_FIGS=4, SIG_FIGS_EXP=2 /

&SLCF PBY=1., QUANTITY='TEMPERATURE', VECTOR=T /
&SLCF PBZ=2., QUANTITY='VELOCITY', VECTOR=T /
&SLCF AGL_SLICE=0.1, QUANTITY='LEVEL SET VALUE' /
&SLCF PBZ=0.1, QUANTITY='HRRPUV', CELL_CENTERED=T/

&BNDF QUANTITY='BURNING RATE' /

&DEVC ID='x', QUANTITY='BURNING RATE', SPATIAL_STATISTIC='MAXLOC X', XB=-20,220,-5,0,0,5 /

&TAIL /
