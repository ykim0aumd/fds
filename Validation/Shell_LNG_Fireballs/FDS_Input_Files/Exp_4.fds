&HEAD CHID='Exp_4', TITLE='Shell LNG Fireball' /

&TIME T_END=10. /
&DUMP DT_HRR=0.1, DT_DEVC=0.1 /

&MESH IJK=40,40,40, XB=-20,20,-20,20,0,40, MULT_ID='mesh' /
&MULT ID='mesh', DX=40., DY=40., DZ=40., I_LOWER=-1, I_UPPER=3, J_LOWER=-1, J_UPPER=1, K_UPPER=4 /

&VENT MB='XMIN', SURF_ID='OPEN' /
&VENT MB='XMAX', SURF_ID='OPEN' /
&VENT MB='YMIN', SURF_ID='OPEN' /
&VENT MB='YMAX', SURF_ID='OPEN' /
&VENT MB='ZMAX', SURF_ID='OPEN' /

&RADI NUMBER_RADIATION_ANGLES=600, PATH_LENGTH=100. /

&SPEC ID='METHANE', LUMPED_COMPONENT_ONLY=T /
&SPEC ID='ETHANE',  LUMPED_COMPONENT_ONLY=T /
&SPEC ID='PROPANE', LUMPED_COMPONENT_ONLY=T /

&SPEC ID='natural gas',
    SPEC_ID(1)='METHANE',  VOLUME_FRACTION(1)= 0.967,
    SPEC_ID(2)='ETHANE',   VOLUME_FRACTION(2)= 0.03,
    SPEC_ID(3)='PROPANE',  VOLUME_FRACTION(3)= 0.001 /

&REAC FUEL ='natural gas',
      SOOT_YIELD=0.005,
      RADIATIVE_FRACTION=0.30 /

&SURF ID='VESSEL', FREE_SLIP=T, COLOR='GRAY' /
&OBST XB=-1.0,1.0,-1.0,1.0,1.,2., SURF_ID='VESSEL' /

&VENT XB=-2.0,2.0,-2.0,2.0,0.,0., SURF_ID='INJECTION' /

&SURF ID='INJECTION', MASS_FLUX(1)=372.3, SPEC_ID(1)='natural gas', TMP_FRONT=-115., RAMP_MF(1)='fuel', COLOR='RED' /

&RAMP ID='fuel', T= 0.00, F=0.0 /
&RAMP ID='fuel', T= 0.20, F=0.0 /
&RAMP ID='fuel', T= 0.21, F=1.0 /
&RAMP ID='fuel', T= 0.41, F=1.0 /
&RAMP ID='fuel', T= 0.42, F=0.0 /

&DEVC ID='rad40',      QUANTITY='RADIATIVE HEAT FLUX GAS', XYZ= 40.0,0.,1.5, ORIENTATION=-1,0,0 /
&DEVC ID='rad40_tmax', QUANTITY='RADIATIVE HEAT FLUX GAS', XYZ= 40.0,0.,1.5, ORIENTATION=-1,0,0, TEMPORAL_STATISTIC='MAX TIME' /
&DEVC ID='rad40_max',  QUANTITY='RADIATIVE HEAT FLUX GAS', XYZ= 40.0,0.,1.5, ORIENTATION=-1,0,0, TEMPORAL_STATISTIC='MAX' /
&DEVC ID='rad70',      QUANTITY='RADIATIVE HEAT FLUX GAS', XYZ= 70.0,0.,1.5, ORIENTATION=-1,0,0 /
&DEVC ID='rad70_tmax', QUANTITY='RADIATIVE HEAT FLUX GAS', XYZ= 70.0,0.,1.5, ORIENTATION=-1,0,0, TEMPORAL_STATISTIC='MAX TIME' /
&DEVC ID='rad70_max',  QUANTITY='RADIATIVE HEAT FLUX GAS', XYZ= 70.0,0.,1.5, ORIENTATION=-1,0,0, TEMPORAL_STATISTIC='MAX' /
&DEVC ID='rad100',     QUANTITY='RADIATIVE HEAT FLUX GAS', XYZ=100.0,0.,1.5, ORIENTATION=-1,0,0 /
&DEVC ID='rad100_tmax',QUANTITY='RADIATIVE HEAT FLUX GAS', XYZ=100.0,0.,1.5, ORIENTATION=-1,0,0, TEMPORAL_STATISTIC='MAX TIME' /
&DEVC ID='rad100_max', QUANTITY='RADIATIVE HEAT FLUX GAS', XYZ=100.0,0.,1.5, ORIENTATION=-1,0,0, TEMPORAL_STATISTIC='MAX' /

&DEVC ID='HRR', Y_ID='y', QUANTITY='HRRPUV', XBP=0.5,0.5,-49.5,49.5,50.5,50.5, POINTS=100, STATISTICS_START=0 /
&CTRL ID='y1', FUNCTION_TYPE='PERCENTILE', INPUT_ID='HRR', PERCENTILE=0.03 /
&CTRL ID='y2', FUNCTION_TYPE='PERCENTILE', INPUT_ID='HRR', PERCENTILE=0.97 /
&CTRL ID='D',  FUNCTION_TYPE='SUBTRACT', INPUT_ID='y2','y1' /
&DEVC ID='D_avg',  QUANTITY='CONTROL VALUE', CTRL_ID='D', XYZ=0,0,0, TEMPORAL_STATISTIC='TIME AVERAGE', UNITS='m' /
&DEVC ID='D_tmax', QUANTITY='CONTROL VALUE', CTRL_ID='D', XYZ=0,0,0, TEMPORAL_STATISTIC='MAX TIME', STATISTICS_START=0, UNITS='s' /
&DEVC ID='D_max',  QUANTITY='CONTROL VALUE', CTRL_ID='D', XYZ=0,0,0, TEMPORAL_STATISTIC='MAX', STATISTICS_START=0, UNITS='m' /

&DEVC ID='Height', QUANTITY='HRRPUV', XB=-50,50,-50,50,0,200, SPATIAL_STATISTIC='CENTROID Z' /

&SLCF PBY=0.1, QUANTITY='TEMPERATURE', CELL_CENTERED=T /
&SLCF PBY=0.1, QUANTITY='VELOCITY', VECTOR=T /
&SLCF PBY=0.1, QUANTITY='INTEGRATED INTENSITY', CELL_CENTERED=T /
&SLCF PBZ=0.1, QUANTITY='INTEGRATED INTENSITY', CELL_CENTERED=T /
&SLCF PBY=0.1, QUANTITY='HRRPUV', CELL_CENTERED=T /
&SLCF PBY=0.1, QUANTITY='VOLUME FRACTION', SPEC_ID='natural gas', CELL_CENTERED=T /

&BNDF QUANTITY='RADIATIVE HEAT FLUX' /

&TAIL /

