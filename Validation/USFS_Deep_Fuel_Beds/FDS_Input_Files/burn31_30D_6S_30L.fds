&HEAD CHID='burn31_30D_6S_30L', TITLE='USFS Deep Fuel Beds burn31 - 30 cm deep - 6 degree slope - 30 cm spacing' / 
!In EXP this run is clasified as an unsuccessful burn.

! &MESH IJK=30,20,20, XB=-3,9,-4,4,0,6/ single coarse mesh for testing

&MULT ID='m1',DX=3,DY=4,DZ=3,
      I_LOWER=0,I_UPPER=3,
      J_LOWER=0,J_UPPER=1,
      K_LOWER=0,K_UPPER=1 / 16 meshes
&MESH IJK=30,40,30,XB=-3,0,-4,0,0,3, MULT_ID='m1' /

&TIME T_END=180 /
&DUMP DT_SLCF=0.5,DT_PART=0.5 /
&MISC HUMIDITY=76.6,TMPA=23.56 /

&VENT DB='XMIN', SURF_ID='OPEN' /
&VENT DB='XMAX', SURF_ID='OPEN' /
&VENT DB='YMIN', SURF_ID='OPEN' /
&VENT DB='YMAX', SURF_ID='OPEN' /
&VENT DB='ZMAX', SURF_ID='OPEN' /

&SURF ID = 'wet vegetation'
      MATL_ID          = 'GENERIC VEGETATION'
      MOISTURE_FRACTION = 0.115
      SURFACE_VOLUME_RATIO = 8000
      LENGTH           = 0.1
      GEOMETRY         = 'CYLINDRICAL'
      COLOR            = 'SILVER' /

&CATF OTHER_FILES = 'vegetation_model.txt' /

&PART ID='EXCELSIOR', SAMPLING_FACTOR=1, SURF_ID='wet vegetation', PROP_ID='wood image'
      QUANTITIES='PARTICLE TEMPERATURE','PARTICLE MASS','PARTICLE DIAMETER', STATIC=T 
      EMBER_PARTICLE=T, EMBER_DENSITY_THRESHOLD=400,EMBER_VELOCITY_THRESHOLD=1 /

- Vegetative fuel array
&MULT ID='FUEL_ARRAY', DXB=0.3031,0.3031,0,0,0.0319,0.0319, N_UPPER=14 /
&INIT PART_ID='EXCELSIOR', XYZ=0.6762,0.5334,0.0842,
      N_PARTICLES=100, MASS_PER_VOLUME=5.489, DRY=T, RADIUS=0.0762, HEIGHT=0.3048, SHAPE='CYLINDER',MULT_ID='FUEL_ARRAY' /
&INIT PART_ID='EXCELSIOR', XYZ=0.6762,0.3810,0.0842,
      N_PARTICLES=100, MASS_PER_VOLUME=5.489, DRY=T, RADIUS=0.0762, HEIGHT=0.3048, SHAPE='CYLINDER',MULT_ID='FUEL_ARRAY' /
&INIT PART_ID='EXCELSIOR', XYZ=0.6762,0.2286,0.0842,
      N_PARTICLES=100, MASS_PER_VOLUME=5.489, DRY=T, RADIUS=0.0762, HEIGHT=0.3048, SHAPE='CYLINDER',MULT_ID='FUEL_ARRAY' /
&INIT PART_ID='EXCELSIOR', XYZ=0.6762,0.0762,0.0842,
      N_PARTICLES=100, MASS_PER_VOLUME=5.489, DRY=T, RADIUS=0.0762, HEIGHT=0.3048, SHAPE='CYLINDER',MULT_ID='FUEL_ARRAY' /   
&INIT PART_ID='EXCELSIOR', XYZ=0.6762,-0.5334,0.0842,
      N_PARTICLES=100, MASS_PER_VOLUME=5.489, DRY=T, RADIUS=0.0762, HEIGHT=0.3048, SHAPE='CYLINDER',MULT_ID='FUEL_ARRAY' /
&INIT PART_ID='EXCELSIOR', XYZ=0.6762,-0.3810,0.0842,
      N_PARTICLES=100, MASS_PER_VOLUME=5.489, DRY=T, RADIUS=0.0762, HEIGHT=0.3048, SHAPE='CYLINDER',MULT_ID='FUEL_ARRAY' /
&INIT PART_ID='EXCELSIOR', XYZ=0.6762,-0.2286,0.0842,
      N_PARTICLES=100, MASS_PER_VOLUME=5.489, DRY=T, RADIUS=0.0762, HEIGHT=0.3048, SHAPE='CYLINDER',MULT_ID='FUEL_ARRAY' /
&INIT PART_ID='EXCELSIOR', XYZ=0.6762,-0.0762,0.0842,
      N_PARTICLES=100, MASS_PER_VOLUME=5.489, DRY=T, RADIUS=0.0762, HEIGHT=0.3048, SHAPE='CYLINDER',MULT_ID='FUEL_ARRAY' /

- Fine mesh Platform underneath fuel array and out 40 cm on each side
&SURF ID='platform',COLOR='GRAY'/
&OBST XB=0.6,0.8,-1.45,1.45,-0.2,0.0,SURF_ID='platform', MULT_ID='platform' / 
&MULT ID='platform',DXB=0.2,0.2,0,0,0.021,0.021, N_UPPER=22 /

- Ignitor
&RAMP ID='RAMP IGN',T=0,  F=1.0 /
&RAMP ID='RAMP IGN',T=10, F=1.0 /
&RAMP ID='RAMP IGN',T=20, F=0.0 /
&RAMP ID='RAMP IGN',T=180, F=0.0 /

&INIT XB=0.55,0.75,-0.60,0.60,0.05,0.35, HRRPUV=500, RAMP_Q='RAMP IGN' /

&DEVC ID='Fuel Load', QUANTITY='MPUV', PART_ID='EXCELSIOR', SPATIAL_STATISTIC='VOLUME INTEGRAL', XB=-3,9,-4,4,0,6 /

&SLCF PBY=0.0, QUANTITY='VELOCITY', VECTOR=T /
&SLCF PBY=0.0, QUANTITY='TEMPERATURE', CELL_CENTERED=T /
&SLCF PBY=0.0, QUANTITY='HRRPUV', VECTOR=T /

&SLCF PBZ=0.5, QUANTITY='VELOCITY', VECTOR=T /
&SLCF PBZ=0.5, QUANTITY='TEMPERATURE', CELL_CENTERED=T /
&SLCF PBZ=0.5, QUANTITY='HRRPUV', VECTOR=T /

&TAIL /